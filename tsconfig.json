{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@pages/*": ["./src/pages/*"],
      "@scripts/*":["./src/scripts/*"],
      "@styles/*":["./src/styles/*"],
      "@public/*": ["./public/*"],
      "@content/*": ["./src/pages/conent/*"],
    },
  },
}